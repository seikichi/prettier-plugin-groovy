{
  "name": "prettier-plugin-groovy",
  "description": "a groovy prettier plugin",
  "version": "0.0.1",
  "author": "seikichi",
  "bugs": {
    "url": "https://github.com/seikichi/prettier-plugin-groovy/issues"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/prettier": "^1.18.3",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "cross-env": "^5.2.1",
    "jest": "^24.9.0",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/seikichi/prettier-plugin-groovy",
  "keywords": [
    "groovy",
    "prettier"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seikichi/prettier-plugin-groovy"
  },
  "scripts": {
    "build": "run-s build:g4 build:ts",
    "build:g4": "antlr4ts -no-listener src/antlr4/GroovyLexer.g4 src/antlr4/GroovyParser.g4",
    "build:ts": "cross-env NODE_ENV=production tsc -p .",
    "prettier": "prettier --plugin=. --parser=groovy",
    "test": "jest",
    "watch": "tsc -p . --watch"
  }
}
